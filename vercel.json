{
  "version": 2,
  "builds": [
    { "src": "portfolio/index.html", "use": "@vercel/static" },
    { "src": "portfolio/assets/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "portfolio/assets/$1" },
    { "src": "/(.*)", "dest": "portfolio/index.html" }
  ]
}
